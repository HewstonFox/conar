{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "deep-link:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-focus",
    "core:window:allow-unminimize",
    "store:default",
    "updater:default",
    "process:default"
  ]
}
